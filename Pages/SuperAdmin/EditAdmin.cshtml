@page
@model EDPFinal.Pages.SuperAdmin.EditAdminModel
@{
    ViewData["Title"] = "Edit Admin";
}

<h1>Edit Admin</h1>
<form method="post">

    <div asp-validation-summary="All" class="text-danger"></div>


    <div>
        <label class="col-sm-2 col-form-label">Admin Email:</label>
        <input asp-for="MyAdmin.adminEmail" />
        <span asp-validation-for="MyAdmin.adminEmail" class="text-danger"></span>
    </div>
    

    <div class="col-sm-10">
        <input id="edit" type="submit" value="Save" class="btn btn-primary" />
    </div>
</form>
@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
